{
  "name": "whatsapp-news-bot",
  "private": true,
  "scripts": {
    "setup": "npm install --prefix server && npm install --prefix apps/web",
    "dev": "npm run dev --prefix server",
    "dev:server": "npm run dev --prefix server",
    "dev:client": "npm run dev:web",
    "dev:web": "npm run dev --prefix apps/web",
    "start": "npm run start --prefix server",
    "start:server": "npm run start --prefix server",
    "start:client": "npm run start:web",
    "start:web": "npm run start --prefix apps/web",
    "build": "npm run build:server && npm run build:web:static",
    "build:all": "npm run build",
    "build:server": "npm run build --prefix server",
    "build:client": "npm run build:web:static",
    "build:web": "npm run build --prefix apps/web",
    "build:web:static": "node tools/build-web-static.mjs",
    "db:migrate": "npm run migrate --prefix server",
    "db:check": "node tools/check-migrations.mjs",
    "smoke": "node tools/smoke.mjs"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@supabase/supabase-js": "^2.45.0",
    "@tanstack/react-query": "^5.90.20",
    "@whiskeysockets/baileys": "^6.7.9",
    "axios": "^1.6.8",
    "cheerio": "^1.0.0-rc.12",
    "class-variance-authority": "^0.7.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "fast-fuzzy": "^1.12.0",
    "he": "^1.2.0",
    "joi": "^17.11.0",
    "link-preview-js": "^3.2.0",
    "lucide-react": "^0.563.0",
    "nanoid": "^5.0.7",
    "node-cron": "^3.0.3",
    "pino": "^9.2.0",
    "qrcode": "^1.5.4",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-hook-form": "^7.71.1",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.13.0",
    "rss-parser": "^3.13.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.8"
  }
}
